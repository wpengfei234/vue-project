body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6, 
form,input,textarea,p,th,td{ 
    padding: 0; 
    margin: 0;
} 
a{
	text-decoration: none;
	color: #000;
}
table {
    border-collapse: collapse;
    border-spacing: 0 ;
}
img {
	display: block;
    border: 0; 
}
ol,ul{  
    list-style: none; 
}  
h1,h2,h3,h4,h5,h6 { 
    font-weight: normal;
    font-size: 100%; 
}
input{
	border: 0;
	outline: none;
}